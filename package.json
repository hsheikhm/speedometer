{
  "name": "speedometer",
  "version": "1.0.0",
  "description": "A Speedometer built in HTML, CSS, Sass, Materialize, Node, Express, and Angular JS",
  "main": "server.js",
  "scripts": {
    "postinstall": "bower install && grunt",
    "prestart": "npm install",
    "start": "node server.js",
    "restart": "grunt && nodemon server.js",
    "pretest": "npm install",
    "test": "node node_modules/karma/bin/karma start test/karma.conf.js",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor test/protractor-conf.js",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update"
  },
  "devDependencies": {
    "body-parser": "^1.15.0",
    "bower": "^1.3.1",
    "express": "^4.13.4",
    "grunt": "^1.0.1",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-cssmin": "^1.0.1",
    "grunt-contrib-htmlmin": "^1.4.0",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-uglify": "^1.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "karma": "^0.12.16",
    "karma-chrome-launcher": "^0.1.4",
    "karma-firefox-launcher": "^0.1.3",
    "karma-jasmine": "~0.1.0",
    "method-override": "^2.3.5",
    "nodemon": "^1.9.2",
    "protractor": "^2.1.0",
    "tmp": "0.0.23"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hsheikhm/speedometer.git"
  },
  "keywords": [],
  "author": "Hamza Sheikh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hsheikhm/speedometer/issues"
  },
  "homepage": "https://github.com/hsheikhm/speedometer#readme"
}
