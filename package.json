{
  "name": "speedometer",
  "version": "1.0.0",
  "description": "A Speedometer built in HTML, CSS, Sass, Materialize, Node, Express, and Angular JS",
  "main": "server.js",
  "scripts": {
    "postinstall": "bower install && grunt",
    "prestart": "npm install",
    "start": "node server.js",
    "restart": "grunt && nodemon server.js",
    "pretest": "npm install",
    "test": "node node_modules/karma/bin/karma start test/karma.conf.js",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor test/protractor-conf.js",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update"
  },
  "devDependencies": {
    "body-parser": "latest",
    "bower": "latest",
    "express": "latest",
    "grunt": "latest",
    "grunt-contrib-clean": "latest",
    "grunt-contrib-concat": "latest",
    "grunt-contrib-cssmin": "latest",
    "grunt-contrib-jshint": "latest",
    "grunt-contrib-sass": "latest",
    "grunt-contrib-uglify": "latest",
    "grunt-contrib-watch": "latest",
    "karma": "latest",
    "karma-chrome-launcher": "latest",
    "karma-firefox-launcher": "latest",
    "karma-jasmine": "latest",
    "method-override": "latest",
    "nodemon": "latest",
    "protractor": "latest",
    "tmp": "latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hsheikhm/speedometer.git"
  },
  "keywords": [],
  "author": "Hamza Sheikh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hsheikhm/speedometer/issues"
  },
  "homepage": "https://github.com/hsheikhm/speedometer#readme"
}
